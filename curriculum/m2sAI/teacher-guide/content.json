    "settings": {
      "table": {
        "numFormat": ".3~f"
      },
      "text": {
        "tools": [
          "bold",
          "italic",
          "list-ul",
          "subscript",
          "superscript",
          "link",
          "new-variable",
          "insert-variable",
          "edit-variable"
        ]
      },
      "drawing": {
        "tools": [
          "select",
          "line",
          "vector",
          "rectangle",
          "ellipse",
          "stamp",
          "stroke-color",
          "fill-color",
          "image-upload",
          "new-variable",
          "insert-variable",
          "edit-variable",
          "group",
          "ungroup",
          "delete"
        ]
      },
      "diagram": {
        "maxTiles": 1
      },
      "graph": {
        "defaultAxisLabels": {
          "bottom": "x",
          "left": "y"
        },
        "emptyPlotIsNumeric": true,
        "scalePlotOnValueChange": true
