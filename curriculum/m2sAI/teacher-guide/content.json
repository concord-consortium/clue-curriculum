{
  "code": "m2sAI",
  "abbrevTitle": "M2SAI",
  "title": "M2Studio AI Teacher Guide",
  "subtitle": "",
  "config": {
    "autoAssignStudentsToIndividualGroups": true,
    "autoSectionProblemDocuments": false,
    "disablePublish": [
      {
        "documentType": "learningLog"
      }
    ],
    "placeholderText": "Enter notes here",
    "defaultDocumentType": "problem",
