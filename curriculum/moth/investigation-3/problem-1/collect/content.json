      {
        "id": "ffP5vaAz9sV-f2d5",
        "title": "Text 12",
        "content": {
          "type": "Text",
          "format": "html",
          "text": [
            "<p><strong>SHARE</strong></p><p>Share your data with your group<strong> </strong>to discuss what you are learning. What are your next steps?</p>"
          ]
        }
      }
