      {
        "id": "_iLxJYcHxoUqCdOw",
        "title": "Text 59",
        "display": "teacher",
        "content": {
          "type": "Text",
          "format": "html",
          "text": [
            "<p>The two shorter sides must be longer than the third or they will not touch to form a triangle.</p>"
          ]
        }
      },
      {
        "id": "mInYsOkq8zSatURe",
        "title": "Text 57",
