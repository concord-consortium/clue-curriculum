        "id": "X9Rkz0vBPrdVsrx-",
        "title": "Text 2",
        "content": {
          "type": "Text",
          "format": "html",
          "text": [
            "<p>The angles on opposite sides of a vertex are called <strong>vertical angles</strong>.</p>"
          ]
        }
