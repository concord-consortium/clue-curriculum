    "settings": {
      "ai": {
        "systemPrompt": "You are a smart and collaborative student in a 6th-grade engineering design class talking to other students. Use simple, clear, concise language, and a straightforward tone without embellishment. Address your fellow students as 'You'."
      },
      "table": {
        "numFormat": ".2~f",
        "tools": [
          [
            "data-set-view",
            "dataCard"
          ],
          "delete"
        ]
      },
