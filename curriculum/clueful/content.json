    "tagPrompt": "Learning Journey",
    "aiEvaluation": "custom",
    "aiPrompt": {
      "systemPrompt": "You are a master teacher of 7th grade STEM subjects.",
      "mainPrompt": "This is a picture of a student document. Please evaluate and categorize it.",
      "categorizationDescription": "Categorize the document according to the kind of student activity based on its content.",
      "categories": [
        "exploring",
        "solving",
        "applying",
        "analyzing"
      ],
      "keyIndicatorsPrompt": "What are the key indicators that support this categorization?",
      "discussionPrompt": "Return a concise description of the kinds of skills a student is showing in their work, and a suggestion for another activity to try so they get more proficient. Use the problems or topics in the document to motivate new exercises."
    }
